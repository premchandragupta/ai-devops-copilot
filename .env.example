# REPO-ROOT .env.example (SAFE TO COMMIT)
POSTGRES_URL=postgres://postgres:postgres@localhost:5432/adc
AUDIT_LOG_URL=http://localhost:4003/events

# Slack
SLACK_BOT_TOKEN=__SET_IN_LOCAL_ENV__
